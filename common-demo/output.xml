<?xml version="1.0" encoding="UTF-8"?>

<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">  
  <modelVersion>4.0.0</modelVersion>  
  <groupId>com.xsyx.fd.test</groupId>  
  <artifactId>xsyx-fd-test</artifactId>  
  <version>1.0-SNAPSHOT</version>  
  <!--test-->  
  <properties> 
    <kylin-client-token.version>3.2.2-RELEASE</kylin-client-token.version>  
    <springfox-swagger2.version>2.9.2</springfox-swagger2.version>  
    <project.main.class>com.frxs.fd.Application</project.main.class>  
    <frxs-fd-hbase-api.version>1.0.1-RELEASE</frxs-fd-hbase-api.version>  
    <druid-version>1.1.21</druid-version>  
    <mybaits-plus.version>3.4.2</mybaits-plus.version>  
    <hbase.version>1.2.0-cdh5.16.1</hbase.version>  
    <grok.version>0.1.5</grok.version>  
    <mysql-8.version>8.0.18</mysql-8.version>  
    <freemarker.version>2.3.31</freemarker.version>  
    <hadoop.version>2.6.0-cdh5.16.1.1</hadoop.version>  
    <map-struct.version>1.2.0.Final</map-struct.version>  
    <frxs-fd-hbase-spring.version>1.0.1-RELEASE</frxs-fd-hbase-spring.version>  
    <log4j2.version>2.17.0</log4j2.version>  
    <phoenix.version>4.14.2-cdh5.14.4-xsyx</phoenix.version>  
    <kafka-clients.version>2.2.0</kafka-clients.version> 
  </properties>  
  <dependencies> 
    <dependency> 
      <groupId>com.dangdang</groupId>  
      <artifactId>elastic-job-lite-core</artifactId>  
      <version>2.1.5-frxs</version>  
      <exclusions> 
        <exclusion> 
          <artifactId>log4j</artifactId>  
          <groupId>log4j</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>slf4j-log4j12</artifactId>  
          <groupId>org.slf4j</groupId> 
        </exclusion> 
      </exclusions> 
    </dependency>  
    <dependency> 
      <groupId>com.dangdang</groupId>  
      <artifactId>elastic-job-lite-spring</artifactId>  
      <version>2.1.5-frxs</version> 
    </dependency>  
    <dependency> 
      <groupId>com.frxs.foundation.base</groupId>  
      <artifactId>frxs-fd-hbase-spring</artifactId>  
      <version>${frxs-fd-hbase-spring.version}</version>  
      <exclusions> 
        <exclusion> 
          <artifactId>log4j</artifactId>  
          <groupId>log4j</groupId> 
        </exclusion> 
      </exclusions> 
    </dependency>  
    <dependency> 
      <groupId>org.springframework.boot</groupId>  
      <artifactId>spring-boot-starter-web</artifactId>  
      <version>2.0.3.RELEASE</version>  
      <exclusions> 
        <exclusion> 
          <artifactId>spring-boot-starter-logging</artifactId>  
          <groupId>org.springframework.boot</groupId> 
        </exclusion> 
      </exclusions> 
    </dependency>  
    <dependency> 
      <groupId>com.google.guava</groupId>  
      <artifactId>guava</artifactId>  
      <version>27.1-jre</version> 
    </dependency>  
    <dependency> 
      <groupId>org.apache.phoenix</groupId>  
      <artifactId>phoenix-core</artifactId>  
      <version>${phoenix.version}</version>  
      <exclusions> 
        <exclusion> 
          <artifactId>guava</artifactId>  
          <groupId>com.google.guava</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>hadoop-yarn-common</artifactId>  
          <groupId>org.apache.hadoop</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>joda-time</artifactId>  
          <groupId>joda-time</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>hbase-protocol</artifactId>  
          <groupId>org.apache.hbase</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>netty-all</artifactId>  
          <groupId>io.netty</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>hbase-annotations</artifactId>  
          <groupId>org.apache.hbase</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>hbase-client</artifactId>  
          <groupId>org.apache.hbase</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>hbase-common</artifactId>  
          <groupId>org.apache.hbase</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>gson</artifactId>  
          <groupId>com.google.code.gson</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>netty</artifactId>  
          <groupId>io.netty</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>slf4j-log4j12</artifactId>  
          <groupId>org.slf4j</groupId> 
        </exclusion>  
        <exclusion> 
          <artifactId>log4j</artifactId>  
          <groupId>log4j</groupId> 
        </exclusion> 
      </exclusions> 
    </dependency>  
    <dependency> 
      <groupId>com.frxs.foundation.base</groupId>  
      <artifactId>frxs-fd-elasticsearch-spring</artifactId>  
      <version>1.4.3-CMDB-RELEASE</version>  
      <exclusions> 
        <exclusion> 
          <artifactId>log4j-api</artifactId>  
          <groupId>org.apache.logging.log4j</groupId> 
        </exclusion> 
      </exclusions> 
    </dependency>  
    <dependency> 
      <groupId>com.xsyx.fd.api</groupId>  
      <artifactId>tracking-log-api</artifactId>  
      <version>1.0.0-SNAPSHOT</version>  
      <exclusions> 
        <exclusion> 
          <artifactId>xsyx-fd-log-api</artifactId>  
          <groupId>com.xsyx.foundation.base</groupId> 
        </exclusion> 
      </exclusions> 
    </dependency>  
    <dependency> 
      <groupId>com.xsyx.foundation.base</groupId>  
      <artifactId>xsyx-fd-log-api</artifactId>  
      <version>1.0.0-SNAPSHOT</version> 
    </dependency>  
    <!--ext-plugin add log start.-->
    <dependency>
      <groupId>org.slf4j</groupId>
      <artifactId>slf4j-api</artifactId>
      <version>1.7.25</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.apache.logging.log4j</groupId>
      <artifactId>log4j-api</artifactId>
      <version>2.17.0</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.apache.logging.log4j</groupId>
      <artifactId>log4j-core</artifactId>
      <version>2.17.0</version>
      <scope>compile</scope>
    </dependency>
    <dependency>
      <groupId>org.apache.logging.log4j</groupId>
      <artifactId>log4j-slf4j-impl</artifactId>
      <version>2.17.0</version>
      <scope>compile</scope>
    </dependency>
    <!--ext-plugin add log end.-->
  </dependencies>  
  <build> 
    <plugins>
      <plugin> 
        <artifactId>maven-shade-plugin</artifactId>  
        <version>3.2.4</version>  
        <executions> 
          <execution> 
            <phase>package</phase>  
            <goals> 
              <goal>shade</goal> 
            </goals>  
            <configuration> 
              <transformers> 
                <transformer implementation="org.apache.maven.plugins.shade.resource.ManifestResourceTransformer"> 
                  <mainClass>com.xsyx.fd.test.two.App</mainClass> 
                </transformer> 
              </transformers> 
            </configuration> 
          </execution> 
        </executions> 
      </plugin>  
      <plugin> 
        <groupId>org.springframework.boot</groupId>  
        <artifactId>spring-boot-maven-plugin</artifactId>  
        <executions> 
          <execution> 
            <goals> 
              <goal>repackage</goal> 
            </goals> 
          </execution> 
        </executions> 
      </plugin> 
    </plugins> 
  </build> 
</project>
